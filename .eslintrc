{
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"quotes": [
			2,
			"single"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		]
	},
	"ecmaFeatures": {
		"arrowFunctions": true,
		"forOf": true,
		"jsx": true
	},
	"env": {
		"es6": true,
		"node": true,
		"browser": true,
		"mocha": true,
		"mongo": true
	},
	"extends": "eslint:recommended"
}
