{
    "server": {
        "debug": false,
        "accessLog": "/var/log/hipache.log",
        "workers": 1,
        "maxSockets": 1024,
        "deadBackendTTL": 30,
        "tcpTimeout": 15,
        "retryOnError": 0,
        "deadBackendOn500": true,
        "httpKeepAlive": true,
        "lruCache": true
    },
    "http": {
        "enabled": false
    },
    "https": {
    	"bind" : "127.0.0.1",
     	"port" : 443,
    	"cert" : "config/server.crt", 
    	"key"  : "config/server.key"
    },
    "user": "riox",
    "group": "riox",
    "driver": "redis://redis.dev.riox.internal:6379/gateway"
}
